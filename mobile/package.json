{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android & npm run watch",
    "androidDependencies": "cd ./android && ./gradlew :app:dependencies",
    "androidDeploy": "cd ./android && ./gradlew assembleRelease && ./gradlew installRelease && cd ../",
    "androidRelease": "react-native run-android --variant=release",
    "apk": "cd ./android && ./gradlew assembleRelease && open ./app/build/outputs/apk/release && cd ../",
    "ios": "react-native run-ios & npm run watch",
    "keystore:facebook:dev": "keytool -exportcert -alias androiddebugkey -keystore ~/.android/debug.keystore  -storepass android -keypass android",
    "keystore:facebook:prod": "keytool -exportcert -alias androiddebugkey -keystore ./mobile/android/app/my-release-key.keystore  -storepass android -keypass android",
    "keystore:google:dev": "keytool -list -v -alias androiddebugkey -keystore ~/.android/debug.keystore -storepass android -keypass android",
    "keystore:google:prod": "keytool -list -v -alias androiddebugkey -keystore ./mobile/android/app/my-release-key.keystore -storepass android -keypass android",
    "keystore:prod": "keytool -genkey -v -keystore my-release-key.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000",
    "link": "react-native linknopod && react-native link && git checkout HEAD ./ios/Podfile",
    "postinstall": "cp -r ../common ./common-mobile && rm -rf node_modules/react-native-facebook-login/.git",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "detox test",
    "watch": "cd ./bin && sh ./common-watch.sh"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.2.1",
    "@react-native-community/netinfo": "^1.2.3",
    "convert-string": "^0.1.0",
    "events": "^1.1.1",
    "flux-react-dispatcher": "^1.2.5",
    "inputmask-core": "^2.2.0",
    "js-base64": "^2.5.1",
    "lottie-react-native": "^2.5.11",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-native": "0.59.1",
    "react-native-animatable": "^1.3.1",
    "react-native-auto-height-image": "^1.1.0",
    "react-native-bottomsheet": "^1.9.5",
    "react-native-branch": "2.3.3",
    "react-native-bullet-train": "0.0.42",
    "react-native-device-info": "^1.0.0",
    "react-native-fabric": "^0.5.2",
    "react-native-fabric-crashlytics": "^0.1.8",
    "react-native-facebook-login": "^1.6.1",
    "react-native-firebase": "5.2.1",
    "react-native-globals": "^0.50.0",
    "react-native-google-signin": "^1.2.1",
    "react-native-interactable": "git+https://github.com/SolidStateGroup/react-native-interactable#master",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-modal-datetime-picker": "^6.1.0",
    "react-native-navigation": "2.18.1",
    "react-native-permissions": "^1.1.1",
    "react-native-svg": "^9.3.3",
    "react-native-swiper": "^1.5.14",
    "react-native-vector-icons": "^6.4.1",
    "react-native-view-overflow": "^0.0.4",
    "react-native-webview": "^5.3.1",
    "react-native-xml2js": "^1.0.3",
    "ssg-util": "0.0.2",
    "timers": "^0.1.1",
    "xml-js": "^1.6.9"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/runtime": "7.3.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.4.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "jest": "24.4.0",
    "metro-react-native-babel-preset": "0.53.0",
    "mocha": "^5.0.4",
    "prettier": "1.15.2",
    "react-native-linknopod": "^2.0.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/boilerplate.app/",
        "build": "xcodebuild -workspace ios/boilerplate.xcworkspace -scheme boilerplate -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8 Plus"
      }
    }
  }
}
